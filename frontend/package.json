{
  "name": "frontend",
  "private": true,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint . --fix --ignore-pattern .gitignore",
    "lint:check": "eslint . --max-warnings 0 --ignore-pattern .gitignore || (echo 'Linter failed' && exit 1)",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/ || (echo 'Code is not formatted' && exit 1)"
  },
  "dependencies": {
    "@mdi/font": "7.4.47",
    "vue": "^3.4.31",
    "vuetify": "^3.6.14"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@tsconfig/node22": "^22.0.0",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.3.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-oxlint": "^0.15.7",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.14.0",
    "npm-run-all2": "^7.0.1",
    "prettier": "^3.4.2",
    "typescript": "~5.6.3",
    "typescript-eslint": "^8.20.0",
    "unplugin-fonts": "^1.1.1",
    "unplugin-vue-components": "^0.27.2",
    "vite": "^5.4.10",
    "vite-plugin-vue-devtools": "^7.7.0",
    "vite-plugin-vuetify": "^2.0.3",
    "vue-tsc": "^2.1.10"
  }
}
